{
  "name": "rnSimpleBlogReduxV2",
  "version": "0.0.1",
  "private": true,
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "Common/**/*.{js|jsx}",
      "Web/src/js/**/*.{js|jsx}",
      "!**/node_modules/**",
      "!**/constants.js",
      "!**/appConfig.js",
      "!**/env.json",
      "!Common/env.json",
      "!Common/awsConfig.js",
      "!Common/reducers/root-reducer.js",
      "!Common/stores/*.js",
      "!Common/actions/clients.js"
    ],
    "testPathIgnorePatterns": [
      "/(node_modules|scripts)/"
    ],
    "setupFiles": [
      "./__mocks__/setup.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "Common"
    ],
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "./node_modules/react-native/packager/packager.sh start --reset-cache",
    "test": "NODE_ENV=test PLATFORM=web TEST=true jest",
    "test-web": "NODE_ENV=test PLATFORM=web TEST=true jest --coverage --watch",
    "test-native": "NODE_ENV=development PLATFORM=native TEST=true jest --coverage --watch",
    "build:ios": "react-native bundle --entry-file='index.ios.js' --bundle-output='./ios/YourAppName/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "unbuild": "rm -rf node_modules || rd /s /q node_modules",
    "build": "npm run unbuild && npm install",
    "local": "concurrently \"react-native run-ios\" \"cd Web && ./scripts/runDevServer.sh\"",
    "webpack": "cd Web && ./scripts/runDevServer.sh",
    "build-web-client-prod": "cd Web && ./scripts/buildWebClientProd.sh",
    "build-web-server-prod": "cd Web && ./scripts/buildWebServerProd.sh",
    "start-web-prod": "cd Web && ./scripts/runProdServer.sh",
    "deploy-native": "code-push release-react LGSSearch ios",
    "deploy-native-promote": "code-push promote LGSSearch Staging Production",
    "deploy-native-rollback": "code-push rollback LGSSearch Production"
  },
